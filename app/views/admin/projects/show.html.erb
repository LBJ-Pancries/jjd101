<div class="col-md-12">
  <div class="group">
    <% if current_user && current_user == @project.user %>
      <%= link_to("Edit", edit_admin_project_path(@project), class: "btn btn-primary pull-right")%>
    <% end %>
  </div>
  <h2><%= @project.title %></h2>
  <p><%= @project.description %></p>

  <table class="table table-hover">
    <% @subprojects.each do |subproject| %>
    <tr>
      <td><%= subproject.id %></td>
      <td><%= subproject.titlev%></td>
    </tr>
    <% end %>
  </table>
</div>
