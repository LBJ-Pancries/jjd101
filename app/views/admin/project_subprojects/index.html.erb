<h1><%= @project.title %>/ Subprojects </h1>
<p class="text-right">
  <%= link_to "New Subprojects", new_admin_project_subproject_path(@project), :class => "btn btn-primary"%>
</p>

<table class="table">
  <tr>
    <th>ID</th>
    <th>Title</th>
    <th>Actions</th>
  </tr>
  <% @subprojects.each do |subproject| %>
    <tr>
      <td><%= subproject.id %></td>
      <td><%= subproject.title %></td>
      <td>
        <%= link_to "Edit", edit_admin_project_subproject_path(@project, subproject), :class => "btn btn-default" %>
        <%= link_to "Delete", admin_project_subproject_path(@project, subproject), :method => "delete", :data => { :confirm => "Are you sure?"}, :class => "btn btn-danger" %>
      </td>
    </tr>
  <% end %>
</table>
