<div class="col-md-12">
  <h2>项目列表</h2>
  <table class="table table-hover">
    <thead>
      <tr>
        <th>ID</th>
        <th>Title</th>
        <th></th>
      </tr>
    </thead>
    <tbody>
      <% @projects.each do |project| %>
        <tr>
          <td><%= project.id %></td>
          <td><%= link_to(project.title, project_path(project))%></td>
          <td><%= project.created_at %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>
